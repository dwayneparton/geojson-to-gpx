{
    "name": "@dwayneparton/geojson-to-gpx",
    "version": "0.0.23",
    "description": "Converts GeoJson to GPX",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "start": "npx tsc -w",
        "build": "npx tsc",
        "build:min": "webpack --config ./webpack.config.ts --mode=production --env=production --progress",
        "test": "jest",
        "lint": "npx eslint ./src"
    },
    "keywords": [
        "geojson",
        "gpx"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dwayneparton/geojson-to-gpx.git"
    },
    "bugs": {
        "url": "https://github.com/dwayneparton/geojson-to-gpx/issues"
    },
    "homepage": "https://github.com/dwayneparton/geojson-to-gpx",
    "author": "Dwayne Parton",
    "license": "MIT",
    "browserslist": [
        "defaults and supports es6-module"
    ],
    "devDependencies": {
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/preset-env": "^7.22.4",
        "@babel/preset-typescript": "^7.21.5",
        "@types/geojson": "^7946.0.10",
        "@types/jest": "^29.5.2",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "babel-loader": "^9.1.2",
        "browserslist": "^4.21.7",
        "eslint": "^8.42.0",
        "eslint-config-airbnb": "^19.0.4",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.2"
    }
}
